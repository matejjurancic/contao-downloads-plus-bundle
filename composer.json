{
    "name": "matejjurancic/contao-downloads-plus-bundle",
    "description": "Some additional functionality added to Contao Downloads Content Element.",
    "license": "LGPL-3.0-or-later",
    "type": "contao-bundle",
    "authors": [
        {
            "name": "Matej Jurančič",
            "homepage": "https://github.com/matejjurancic",
            "role": "Developer"
        }
    ],
    "support": {
        "issues": "https://github.com/matejjurancic/contao-downloads-plus-bundle/issues",
        "source": "https://github.com/matejjurancic/contao-downloads-plus-bundle",
        "docs": "https://github.com/matejjurancic/contao-downloads-plus-bundle"
    },
    "require": {
        "php": "^8.1 || ^8.2",
        "contao/core-bundle": "^4.13"
    },
    "require-dev": {
        "contao/easy-coding-standard": "^4.0",
        "contao/manager-plugin": "^2.3.1",
        "roave/security-advisories": "dev-latest"
    },
    "conflict": {
        "contao/core": "*",
        "contao/manager-plugin": "<2.0 || >=3.0"
    },
    "autoload": {
        "psr-4": {
            "MatejJurancic\\ContaoDownloadsPlusBundle\\": "src/"
        }
    },
    "extra": {
        "contao-manager-plugin": "MatejJurancic\\ContaoDownloadsPlusBundle\\ContaoManager\\Plugin"
    }
}
